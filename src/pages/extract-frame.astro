	<script>
		// Listen for events from FrameExtractor component to hide/show header
		document.addEventListener('DOMContentLoaded', function() {
			const pageHeader = document.getElementById('page-header');
			
			// Function to toggle header visibility
			function toggleHeader(show: boolean) {
				if (pageHeader) {
					if (show) {
						pageHeader.style.display = 'block';
						pageHeader.style.opacity = '1';
						pageHeader.style.transform = 'translateY(0)';
					} else {
						pageHeader.style.opacity = '0';
						pageHeader.style.transform = 'translateY(-10px)';
						setTimeout(() => {
							pageHeader.style.display = 'none';
						}, 300);
					}
				}
			}
			
			// Listen for custom events from FrameExtractor
			document.addEventListener('frameExtractorViewChange', function(event: CustomEvent) {
				const isLandingView = event.detail.currentView === 'landing';
				toggleHeader(isLandingView);
			});
		});
	</script>

