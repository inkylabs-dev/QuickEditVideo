---
import Layout from '../layouts/Layout.astro';
import FrameExtractor from '../components/FrameExtractor.tsx';
import Adsterra from '../components/Adsterra.astro';
import RecommendedTools from '../components/RecommendedTools.astro';
---

<Layout
	title="Extract Video Frame - Capture Still From Video, Save Video Screenshot | QuickEditVideo"
	description="Extract video frame online free! Capture still from video and save video screenshot instantly. Extract frames from video at specific times. Works with MP4, AVI, MOV without upload."
	keywords="extract video frame, capture still from video, save video screenshot, video to image, extract frames from video online, video frame capture, video screenshot tool, capture video frame online, frame extraction tool online free, video to png"
	ogTitle="Free Online Frame Extractor - Extract Screenshots from Videos Instantly"
	ogDescription="Professional frame extraction tool that works in your browser. Extract frames from videos at specific times or intervals. Supports all major video formats. No uploads, completely private and secure."
	canonicalUrl="https://quickeditvideo.com/extract-frame"
	showBreadcrumbs={true}
	currentPage="Extract Frames"
>
	<!-- Structured Data -->
	<script type="application/ld+json">
	{
		"@context": "https://schema.org",
		"@type": "WebApplication",
		"name": "Frame Extractor - QuickEditVideo",
		"description": "Free online frame extraction tool that captures screenshots from videos at specific times",
		"url": "https://quickeditvideo.com/extract-frame",
		"applicationCategory": "MultimediaApplication",
		"operatingSystem": "Any",
		"browserRequirements": "HTML5, WebAssembly support",
		"offers": {
			"@type": "Offer",
			"price": "0",
			"priceCurrency": "USD"
		},
		"featureList": [
			"Extract frames from video files",
			"PNG and JPG format support",
			"Client-side processing (no uploads)",
			"Supports all major video formats",
			"Privacy-focused frame extraction",
			"Real-time progress tracking",
			"Single time or range extraction",
			"Customizable time intervals"
		],
		"provider": {
			"@type": "Organization",
			"name": "QuickEditVideo",
			"url": "https://quickeditvideo.com"
		}
	}
	</script>

	<main class="min-h-screen bg-gray-50">
		<!-- Page Header -->
		<header id="page-header" class="bg-white border-b border-gray-200 py-6 transition-all duration-300">
			<div class="max-w-6xl mx-auto px-4 md:px-6">
				<h1 class="text-3xl font-bold text-gray-900">Extract Video Frames</h1>
				<p class="text-gray-600 mt-2">Capture screenshots and stills from videos</p>
			</div>
		</header>		<!-- Main Frame Extraction Tool -->
		<section class="p-4 md:p-6 w-full">
			<div class="max-w-6xl mx-auto w-full">
				<FrameExtractor client:load />
			</div>
		</section>

		<Adsterra />

		<!-- Quick Help -->
		<section id="quick-help" class="px-4 md:px-6 pb-16">
			<div class="max-w-6xl mx-auto">
				<h3 class="text-2xl font-semibold text-gray-900 mb-8 text-center">Quick Guide</h3>
				
				<div class="grid grid-cols-1 md:grid-cols-3 gap-6">
					<!-- Step 1: Upload -->
					<article class="bg-white rounded-lg border border-gray-200 hover:border-teal-300 hover:shadow-md transition-all p-6 text-center">
						<div class="w-12 h-12 bg-teal-100 rounded-lg flex items-center justify-center mb-4 mx-auto">
							<svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" class="text-teal-600" role="img" aria-label="Video upload icon">
								<path d="M14.5 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7.5L14.5 2z"/>
								<polyline points="14,2 14,8 20,8"/>
								<path d="M10 15.5L16 12L10 8.5V15.5Z"/>
							</svg>
						</div>
						<div class="w-8 h-8 bg-teal-500 rounded-full flex items-center justify-center text-white font-semibold text-sm mb-3 mx-auto">1</div>
						<h4 class="font-semibold text-gray-900 mb-2">Upload Video</h4>
						<p class="text-sm text-gray-600">Upload your video file (supports MP4, WebM, AVI, MOV)</p>
					</article>

					<!-- Step 2: Set Times -->
					<article class="bg-white rounded-lg border border-gray-200 hover:border-teal-300 hover:shadow-md transition-all p-6 text-center">
						<div class="w-12 h-12 bg-teal-100 rounded-lg flex items-center justify-center mb-4 mx-auto">
							<svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" class="text-teal-600" role="img" aria-label="Time selection icon">
								<circle cx="12" cy="12" r="10"/>
								<polyline points="12,6 12,12 16,14"/>
							</svg>
						</div>
						<div class="w-8 h-8 bg-teal-500 rounded-full flex items-center justify-center text-white font-semibold text-sm mb-3 mx-auto">2</div>
						<h4 class="font-semibold text-gray-900 mb-2">Set Times</h4>
						<p class="text-sm text-gray-600">Choose single time or time range with intervals</p>
					</article>

					<!-- Step 3: Download -->
					<article class="bg-white rounded-lg border border-gray-200 hover:border-teal-300 hover:shadow-md transition-all p-6 text-center">
						<div class="w-12 h-12 bg-teal-100 rounded-lg flex items-center justify-center mb-4 mx-auto">
							<svg width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" class="text-teal-600" role="img" aria-label="Download frames icon">
								<path d="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4"/>
								<polyline points="7,10 12,15 17,10"/>
								<line x1="12" y1="15" x2="12" y2="3"/>
							</svg>
						</div>
						<div class="w-8 h-8 bg-teal-500 rounded-full flex items-center justify-center text-white font-semibold text-sm mb-3 mx-auto">3</div>
						<h4 class="font-semibold text-gray-900 mb-2">Download Frames</h4>
						<p class="text-sm text-gray-600">Extract and download individual frames as images</p>
					</article>
				</div>
			</div>
		</section>

		<!-- Content Section for SEO with Sidebar -->
		<section class="bg-gray-50 py-16 px-4 md:px-6">
			<div class="max-w-7xl mx-auto">
				<div class="grid grid-cols-1 lg:grid-cols-4 gap-8">
					<!-- Main Content -->
					<div class="lg:col-span-3">
				<!-- Introduction -->
				<article class="mb-16">
					<h2 class="text-3xl font-bold text-gray-900 mb-6">The Frame Extractor That Makes Sense</h2>
					<div class="prose prose-lg max-w-none text-gray-700 space-y-4">
						<p>
							Need to grab a screenshot from a video? Whether you're creating thumbnails, extracting reference images, capturing key moments for presentations, or building a video storyboard, you shouldn't need complex software just to pull frames from your videos.
						</p>
						<p>
							Our frame extractor does exactly what you need. Upload your video, specify the exact time or time range you want, and download high-quality PNG or JPG frames instantly. Everything processes right in your browser—no uploads to our servers, no software downloads, no registration hassles.
