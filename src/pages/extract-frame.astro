				</section>

				<!-- Use Cases -->
				<section class="mb-16">
					<h3 class="text-2xl font-semibold text-gray-900 mb-6">Perfect For</h3>
					<div class="grid grid-cols-1 md:grid-cols-2 gap-6">
						<div class="bg-white rounded-lg p-6 shadow-sm border border-gray-200">
							<h4 class="font-medium text-gray-900 mb-2">Content Creation</h4>
