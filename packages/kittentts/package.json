{
  "name": "@quickeditvideo/kittentts",
  "version": "0.1.0",
  "description": "KittenTTS JavaScript implementation for browser-based text-to-speech synthesis",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm run build:model && pnpm run build:code",
    "build:model": "pnpm exec node scripts/download-model.js",
    "build:code": "tsc && rollup -c",
    "dev": "tsc --watch",
    "clean": "rm -rf dist assets",
    "prepublishOnly": "pnpm run clean && pnpm run build:with-download"
  },
  "keywords": [
    "tts",
    "text-to-speech",
    "kitten",
    "onnx",
    "speech-synthesis",
    "browser",
    "webassembly"
  ],
  "author": "QuickEditVideo",
  "license": "MIT",
  "dependencies": {
    "onnxruntime-web": ">=1.22.0",
    "phonemizer": "^1.2.1"
  },
  "devDependencies": {
    "@huggingface/hub": "^2.4.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^22.10.2",
    "jszip": "^3.10.1",
    "npyjs": "^1.0.4",
    "rollup": "^4.28.1",
    "rollup-plugin-copy": "^3.5.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "onnxruntime-web": ">=1.22.0"
  }
}
