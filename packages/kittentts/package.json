{
  "name": "@quickeditvideo/kittentts",
  "version": "0.1.0",
  "description": "KittenTTS JavaScript implementation for browser-based text-to-speech synthesis",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "tts",
    "text-to-speech",
    "kitten",
    "onnx",
    "speech-synthesis",
    "browser",
    "webassembly"
  ],
  "author": "QuickEditVideo",
  "license": "MIT",
  "dependencies": {
    "onnxruntime-web": "^1.22.0",
    "phonemizer": "^1.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^22.10.2",
    "rollup": "^4.28.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "onnxruntime-web": ">=1.22.0"
  }
}