{
  "name": "quickeditvideo",
  "type": "module",
  "version": "0.1.2",
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run build:packages && pnpm run copy:onnxruntime && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest --watch",
    "build:packages": "pnpm --filter @quickeditvideo/kittentts run build && pnpm --filter @quickeditvideo/editor run build",
    "copy:onnxruntime": "mkdir -p public/onnxruntime-web/dist && cp -r node_modules/onnxruntime-web/dist/* public/onnxruntime-web/dist/",
    "release:patch": "pnpm build && pnpm version patch && git push",
    "release:minor": "pnpm build && pnpm version minor && git push",
    "release:major": "pnpm build && pnpm version major && git push",
    "release": "pnpm run release:patch"
  },
  "dependencies": {
    "@mediabunny/mp3-encoder": "^1.27.0",
    "@quickeditvideo/editor": "workspace:*",
    "@quickeditvideo/kittentts": "workspace:*",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@remotion/media": "^4.0.394",
    "@remotion/player": "^4.0.394",
    "@remotion/web-renderer": "^4.0.394",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.562.0",
    "mediabunny": "^1.27.0",
    "mp4box": "^1.5.0",
    "next": "^16.0.10",
    "onnxruntime-web": "^1.23.2",
    "phonemizer": "^1.2.1",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^4.0.11",
    "remotion": "^4.0.394",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19",
    "video.js": "^8.23.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.19.27",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "happy-dom": "^7.8.1",
    "tailwindcss-animate": "^1.0.7",
    "vitest": "^1.6.1"
  },
  "pnpm": {
    "approvedBuilds": [
      "@tensorflow/tfjs-node",
      "esbuild",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "@tensorflow/tfjs-node",
      "core-js",
      "esbuild",
      "protobufjs",
      "sharp"
    ]
  }
}
