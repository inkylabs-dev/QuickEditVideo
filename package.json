{
  "name": "quickeditvideo",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run build:packages && pnpm run copy:onnxruntime && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest --watch",
    "build:packages": "pnpm --filter @quickeditvideo/kittentts run build && pnpm --filter @quickeditvideo/editor run build",
    "copy:onnxruntime": "mkdir -p public/onnxruntime-web/dist && cp -r node_modules/onnxruntime-web/dist/* public/onnxruntime-web/dist/"
  },
  "dependencies": {
    "@mediabunny/mp3-encoder": "^1.27.0",
    "@quickeditvideo/editor": "workspace:*",
    "@quickeditvideo/kittentts": "workspace:*",
    "@remotion/player": "^4.0.394",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.23",
    "jszip": "^3.10.1",
    "mediabunny": "^1.27.0",
    "mp4box": "^1.5.0",
    "next": "^16.0.10",
    "onnxruntime-web": "^1.23.2",
    "phonemizer": "^1.2.1",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^4.0.11",
    "tailwindcss": "^3.4.19",
    "video.js": "^8.23.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.19.27",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "happy-dom": "^7.8.1",
    "vitest": "^1.6.1"
  },
  "pnpm": {
    "approvedBuilds": [
      "@tensorflow/tfjs-node",
      "esbuild",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "@tensorflow/tfjs-node",
      "core-js",
      "esbuild",
      "protobufjs",
      "sharp"
    ]
  }
}
